<Project>
  <PropertyGroup>
    <Authors>Oren Novotny</Authors>
    <Copyright>Copyright (C) Oren Novotny</Copyright>
    <Description>SoapFormatter and related types for .NET Standard</Description>
    <PackageTags>SOAP;formatter;serialization;SoapFormatter</PackageTags>
    <PackageProjectUrl>https://github.com/onovotny/SoapFormatter</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/onovotny/SoapFormatter/master/LICENSE</PackageLicenseUrl>
    <DebugType>embedded</DebugType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All"/>
    <PackageReference Include="Nerdbank.GitVersioning" Version="3.5.103" PrivateAssets="all" />
  </ItemGroup>
</Project>
